/* pages/shop-goods/template-shopInfo.wxss */
.iconShopNameContent{
  width: 100%;
  height: 250rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}
.iconShopNameContent image{
  width: 100%;
  height: 100%;
}
.iconShopNameContent .shopIcon{
  margin-top: 40rpx;
  width: 100rpx;
  height: 100rpx;
  position: absolute;
   border-radius: 50%; 
}
.iconShopNameContent .shopName{
  text-align: center;
  font-size: 14px;
  color: white;
  background-color: rgba(0, 0, 0, 0.3);
  margin-top: 190rpx;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
}